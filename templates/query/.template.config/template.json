{
  "$schema": "http://json.schemastore.org/template",
  "author": "SSW",
  "classifications": [
    ".NET",
    "EF Core",
    "SSW",
    "CQRS",
    "Clean Architecture"
  ],
  "name": "SSW Clean Architecture Query",
  "description": "Creates a new SSW Clean Architecture Query",
  "identity": "SSW.Clean.Architecture.Query.CSharp",
  "groupIdentity": "SSW.Clean.Architecture.Query",
  "shortName": "ssw-ca-query",
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "sourceName": "QueryName",
  "preferNameDirectory": false,
  "sources": [
    {
      "source": "./",
      "target": "./"
    }
  ],
  "symbols": {
    "slnName": {
      "type": "parameter",
      "defaultValue": "FoodBlog",
      "replaces": "FoodBlog",
      "isRequired": false,
      "description": "The name of the solution (used for namespace prefix). e.g. FoodBlog"
    },
    "entityName": {
      "type": "parameter",
      "defaultValue": "EntityName",
      "replaces": "EntityName",
      "fileRename": "EntityName",
      "isRequired": true,
      "description": "The name of the entity. e.g. Customer"
    }
  }
}
